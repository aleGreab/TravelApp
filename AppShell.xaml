<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CarTravel.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CarTravel"
    Shell.FlyoutBehavior="Disabled"
    Title="CarTravel">

    <TabBar>
        <ShellContent Title="Welcome Page" ContentTemplate="{DataTemplate local:MainPage}"/>
        <ShellContent Title="Cars" ContentTemplate="{DataTemplate local:CarPage}"/>
        <ShellContent Title="My Car Lists" ContentTemplate="{DataTemplate local:ListEntryPage}"/>
        <ShellContent Title="Trips" ContentTemplate="{DataTemplate local:TravelPage}"/>
        <ShellContent Title="My Trips" ContentTemplate="{DataTemplate local:TripListEntryPage}"/>
        
    </TabBar>

</Shell>
