<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CarTravel.ListEntryPage"
             Title="ListEntryPage">

    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Add Car List" Clicked="OnCarListAddedClicked" />
    </ContentPage.ToolbarItems>
    <ListView x:Name="listView" Margin="20" ItemSelected="OnListViewItemSelected">
        <ListView.ItemTemplate>
            <DataTemplate>
                <TextCell Text="{Binding Name}" Detail="{Binding Model}" />
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>
</ContentPage>